{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "build-storybook": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "storybook-static/**"
      ]
    }
  }
}